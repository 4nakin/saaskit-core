<VirtualHost 127.0.0.1:80>
    ServerAdmin admin@%(host_string)s 
    ServerName %(host_string)s 

    <Directory /webapp/%(host_string)s/parts>
        Order deny,allow
        Allow from all
    </Directory>
    <Directory /webapp/%(host_string)s/src>
        Order deny,allow
        Allow from all
    </Directory>

    LogLevel debug 
    ErrorLog  /var/log/webapp/main/apache_error.log
    CustomLog /var/log/webapp/main/apache_access.log combined
    
    WSGIDaemonProcess main user=webapp group=www-data threads=25
    WSGIProcessGroup main 
    WSGIScriptAlias / /webapp/%(host_string)s/bin/main_site.wsgi
</VirtualHost>

