<VirtualHost 127.0.0.1:80>
    ServerAdmin admin@yotweets.com 
    ServerName yotweets.com 

    <Directory /webapp/yotweets.com/parts>
        Order deny,allow
        Allow from all
    </Directory>
    <Directory /webapp/yotweets.com/src>
        Order deny,allow
        Allow from all
    </Directory>

    LogLevel debug 
    ErrorLog  /var/log/webapp/main/apache_error.log
    CustomLog /var/log/webapp/main/apache_access.log combined
    
    WSGIDaemonProcess main user=webapp group=www-data threads=25
    WSGIProcessGroup main 
    WSGIScriptAlias / /webapp/yotweets.com/bin/main_site.wsgi
</VirtualHost>

